#: gamedata/game_plws.txt:110
#, kde-format
msgid "Dig Deep"
msgstr "Kaiva syvälle"

#: gamedata/game_plws.txt:119
#, kde-format
msgid "Forgotten Gold"
msgstr "Unohdettu kulta"

#: gamedata/game_plws.txt:122
#, kde-format
msgid "Two of Diamonds"
msgstr "Pari timanttia"

#: gamedata/game_plws.txt:125
#, kde-format
msgid "Suicide Jump"
msgstr "Itsemurhahyppy"

#: gamedata/game_plws.txt:128
#, kde-format
msgid "Easy Access"
msgstr "Helpostisaatava"

#: gamedata/game_plws.txt:131
